<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
   package="org.partner"
    android:versionCode="1"
    android:versionName="1.3"
    >

<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

<application
    android:name="org.partner.Util.Util"
    android:allowBackup="true"
    android:icon="@drawable/partner_logo"
    android:label="@string/app_name"
    android:theme="@style/AksharaTheme" >
    <activity
        android:name="org.partner.activity.Splashscreeen"
        android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
        android:screenOrientation="portrait"
        android:windowSoftInputMode="stateHidden|adjustResize"
         >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="org.partner.activity.MainActivity"
        android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
        android:screenOrientation="portrait"
        android:windowSoftInputMode="adjustPan"
        />
</application>

</manifest>
