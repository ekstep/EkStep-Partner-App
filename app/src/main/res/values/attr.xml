<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextView">
        <attr name="fontAssetName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomEditTextView">
        <attr name="fontAssetNameEditText" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="fontAssetButton" format="string" />
    </declare-styleable>


    <declare-styleable name="ComboSeekBar">
        <attr name="multiline" format="boolean" />
        <attr name="myColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>



</resources>